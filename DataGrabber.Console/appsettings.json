{
  "Serilog": {
    "Using":  [ "Serilog.Sinks.Console", "Serilog.Sinks.File", "Serilog.Sinks.Debug" ],
    "MinimumLevel": "Debug",
    "WriteTo": [
      { "Name": "Console" },
      { "Name": "Debug" },
      { "Name": "File", "Args": { "path": "Logs/log_.log" } }
    ],
    "Properties": {
      "Application": "DataGrabber"
    }
  }
}